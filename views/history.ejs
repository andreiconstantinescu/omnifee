	$(document).ready(function () {
		var sum = 0;
		var currentUnpaidBills = [];

		console.log(currentUnpaidBills);
		$('.bill-pay .price span').each(function (value, key) {
				var billValue = parseFloat($(key).text()).toFixed(2);
				sum += billValue;
				console.log(typeof billValue);
				currentUnpaidBills.push(billValue);
		});
		console.log(currentUnpaidBills);
		$('.bill-total .price h3 span').html(sum);
		$('.single-bill .pay').click(function () {
			var currentBillValue = parseFloat($(this).parent().find('.price span').text());
			var currentBallance = parseFloat($('.balance-number span').text());
			var newBallance = currentBallance - currentBillValue;

				if (newBallance <= 0) {
					alert('Va rugam sa reincarcati pentru a putea continua sa platiti facturi!');
				} else {
					$('.balance-number span').text(newBallance.toFixed(2));
					var c = <%= user.bill1Value %>;
					console.log(typeof c, typeof newBallance);
				}
		});
	})
