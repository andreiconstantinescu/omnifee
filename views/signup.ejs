		function cnpValid(n)
{
  var sum = 0;
  sum += n[0]*2+n[1]*7+n[2]*9+n[3]*1+n[4]*4+n[5]*6+n[6]*3+n[7]*5+n[8]*8+n[9]*2+n[10]*7+n[11]*9;
  sum %= 11;
  if ((sum === 10 && n[12] == 1) || (sum < 10 && sum == n[12]))
    return true
      // alert('Valid');
  else
    return false
      // alert('Invalid');

}
function isValid () {
	console.log('pula')
	var cardType = $.payment.cardType($('.cc-number').val());
  $('[data-numeric]').payment('restrictNumeric');
  $('.cc-number').payment('formatCardNumber');
  $('.cc-exp').payment('formatCardExpiry');
  $('.cc-cvc').payment('formatCardCVC');

  $.fn.toggleInputError = function(erred) {
    this.parent('.form-group').toggleClass('has-error', erred);
    this.parent('.form-group').toggleClass('has-no-error', !erred);
    return this;
  };
	// if (!cardType) {
	// 	throw new Error ('card invalid');
	// }
	$('.cc-number').toggleInputError(!$.payment.validateCardNumber($('.cc-number').val()));
	$('.cc-exp').toggleInputError(!$.payment.validateCardExpiry($('.cc-exp').payment('cardExpiryVal')));
	$('.cc-cvc').toggleInputError(!$.payment.validateCardCVC($('.cc-cvc').val(), cardType));

	//- Determina tipul cardului
	switch(cardType) {
		case 'visa':
			$('.cc-brand').html('<i class="fa fa-cc-visa credit-card"></i>')
			break;
		case 'mastercard':
			$('.cc-brand').html('<i class="fa fa-cc-mastercard credit-card"></i>')
			break;
		case 'discover':
			$('.cc-brand').html('<i class="fa fa-cc-discover credit-card"></i>')
			break;
		case 'amex':
			$('.cc-brand').html('<i class="fa fa-cc-amex credit-card"></i>')
			break;
		// American Express
		// Diners Club
		// UnionPay
		// JCB
		// Visa Electron
		// Maestro
		// Forbrugsforeningen
		// Dankort
		// case n:
		// code block
		// break;
		default:
			console.log(0);
		}
	// $('.cc-brand').text(cardType);




	var cnp = $('.cnp').val()
	$('.cnp').toggleInputError(!cnpValid(cnp));
	$('.mail').toggleInputError(!$('.mail').val());
	$('.pwd1').toggleInputError(!($('.pwd2').val() === $('.pwd1').val()&&$('.pwd1').val().length>=8));
	$('.pwd2').toggleInputError(!($('.pwd2').val() === $('.pwd1').val()&&$('.pwd2').val().length>=8));
	$('.nume').toggleInputError(!$('.nume').val());
	$('.prenume').toggleInputError(!$('.prenume').val());
	$('.telefon').toggleInputError(!$('.telefon').val());
	$('.adresa').toggleInputError(!$('.adresa').val());
	$('.validation').removeClass('text-danger text-success');
	$('.validation').addClass($('.has-error').length ? 'text-danger' : 'text-success');
	$('.terms').toggleInputError(!$('.terms').val());
	if (
	!$('.mail').val() || !($('.pwd2').val() === $('.pwd1').val()&&$('.pwd1').val().length>=8) || !($('.pwd2').val() === $('.pwd1').val()&&$('.pwd2').val().length>=8) || !$('.nume').val() || !cnpValid(cnp) || !$('.prenume').val() || !$('.telefon').val() || !$('.adresa').val() || !$('.terms').val() || !$.payment.validateCardNumber($('.cc-number').val()) || !$.payment.validateCardExpiry($('.cc-exp').payment('cardExpiryVal')) || !$.payment.validateCardCVC($('.cc-cvc').val(), cardType)
) {
	return false;
}
};

